{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "http://strongbox.carlspring.org/npm/metadata/feed",
    "definitions": 
    {
        "time": 
        {
            "type": "object",
            "properties": 
            {
                "modified": 
                {
                    "type": "string",
                    "format": "date-time"
                },

                "created": 
                {
                    "type": "string",
                    "format": "date-time"
                }
            },

            "additionalProperties": 
            {
                "type": "string",
                "format": "date-time"
            }
        },

        "users": 
        {
            "type": "object",
            "additionalProperties": 
            {
                "type": "boolean"
            }
        },

        "packageFeed": 
        {
            "type": "object",
            "patternProperties": 
            {
                "^_": 
                {
                    "description": "Any property starting with _ is valid.",
                    "additionalProperties": true,
                    "additionalItems": true
                }
            },

            "properties": 
            {
                "name": 
                {
                    "type": "string"
                },

                "description": 
                {
                    "type": "string"
                },

                "dist-tags": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/distTags"
                },

                "versions": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/versions"
                },

                "readme": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/readme"
                },

                "maintainers": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/maintainers"
                },

                "time": 
                {
                    "$ref": "#definitions/time"
                },

                "homepage": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/homepage"
                },

                "keywords": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/keywords"
                },

                "repository": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/repository"
                },

                "author": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/person"
                },

                "bugs": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/bugs"
                },

                "license": 
                {
                    "$ref": "http://strongbox.carlspring.org/npm/metadata/common#definitions/license"
                },

                "readmeFilename": 
                {
                    "type": "string"
                },

                "users": 
                {
                    "$ref": "#definitions/users"
                }
            }
        }
    },

    "properties": 
    {
        "packageFeed": 
        {
            "$ref": "#/definitions/packageFeed"
        }
    }
}